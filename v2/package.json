{
  "name": "gentlestudent",
  "version": "2.0.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "watch:client": "next -p 5000 src",
    "watch:server": "babel server --out-dir dist --source-maps --watch",
    "build:client": "next build src",
    "build:server": "babel server --out-dir dist --source-maps",
    "build": "cross-env ENV=production yarn build:client && yarn build:server",
    "dev": "cross-env ENV=development concurrently \"yarn watch:client\" \"yarn watch:server\"",
    "serve": "cd dist && firebase serve -p 3000 --debug -o 0.0.0.0",
    "deploy": "yarn build && cd dist && firebase deploy",
    "logs": "firebase functions:log"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "firebase": "^7.14.2",
    "firebase-admin": "^8.11.0",
    "firebase-functions": "^3.6.1",
    "next": "9.3.1",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "styled-jsx": "^3.2.5",
    "yarn": "^1.22.4"
  },
  "engines": {
    "node": "10"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "prettier": "^2.0.2",
    "rimraf": "^3.0.2"
  }
}
